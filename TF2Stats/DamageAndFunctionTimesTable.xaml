<UserControl
    x:Class="TF2Stats.DamageAndFunctionTimesTable"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TF2Stats"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:Interactivity="using:Microsoft.Xaml.Interactivity" xmlns:Interactions="using:Microsoft.Xaml.Interactions.Core"
    mc:Ignorable="d"
    d:DesignHeight="500"
    d:DesignWidth="400">
    <StackPanel Background="#FFF3EAD9">
        <StackPanel.Resources>
            <SolidColorBrush x:Key="NoDiff" Color="DarkGray"/>
            <Style x:Key="Heading" TargetType="StackPanel">
                <Setter Property="Background" Value="DarkOrange"/>
            </Style>
            <Style x:Key="Label" TargetType="StackPanel">
                <Setter Property="Background" Value="Orange"/>
            </Style>
            <Style x:Key="Percent" TargetType="StackPanel">
                <Setter Property="BorderThickness" Value="0"/>
                <Setter Property="Background" Value="LightBlue"/>
                <Setter Property="Width" Value="50"/>
            </Style>
            <Style x:Key="AltBlock" TargetType="StackPanel">
                <Setter Property="BorderBrush" Value="Black"/>
                <Setter Property="BorderThickness" Value="0.5,0,0,0"/>
                <Setter Property="Width" Value="150"/>
            </Style>
            <Style x:Key="MainValue" TargetType="TextBlock">
                <Setter Property="Width" Value="100"/>
            </Style>
        </StackPanel.Resources>
        <StackPanel Style="{StaticResource Heading}" Grid.ColumnSpan="3">
            <TextBlock x:Name="MainHeading" FontWeight="Bold"  HorizontalAlignment="Stretch" Text="Damage and Function Times" TextWrapping="Wrap" VerticalAlignment="Stretch" TextAlignment="Center"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" >
            <StackPanel.Resources>
                <DataTemplate x:Key="rowBody">
                    <TextBlock x:Name="CloseRamp" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                            Text="{Binding Name}" Style="{StaticResource MainValue}"/>
                </DataTemplate>
            </StackPanel.Resources>
            <StackPanel Style="{StaticResource ResourceKey=Label}">
                <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                           Text="" Width="200" />
            </StackPanel>
            <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                       Text="" Style="{StaticResource MainValue}"/> <!--<ContentControl ContentTemplate="{StaticResource rowBody}"/>-->

            <ItemsControl ItemsSource="{Binding Alts}" >
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Horizontal"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource AltBlock}">
                            <ContentControl ContentTemplate="{StaticResource rowBody}"/>
                        </StackPanel>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>

        </StackPanel>

        <!-- TODO  -->
        <!-- TODO Shot type 	Projectile -->
        <!-- TODO Damage type 	Explosive -->
        <!-- TODO Ranged or Melee damage? 	Ranged -->


        <StackPanel Style="{StaticResource Heading}" Grid.ColumnSpan="3">
            <TextBlock x:Name="DamageHeading" HorizontalAlignment="Stretch" Text="Damage" TextWrapping="Wrap" VerticalAlignment="Stretch" TextAlignment="Center"/>
        </StackPanel>
        
        <StackPanel Orientation="Horizontal" Visibility="{Binding CloseRampVisibility}">
            <StackPanel.Resources>
                <DataTemplate x:Key="rowBody">
                    <StackPanel Orientation="Horizontal">
                        <Interactivity:Interaction.Behaviors>
                            <Interactions:DataTriggerBehavior Binding="{Binding CloseRampDiff}" ComparisonCondition="Equal" Value="False">
                                <Interactions:ChangePropertyAction TargetObject="{Binding ElementName=CloseRampPercent}" PropertyName="Foreground" Value="{StaticResource NoDiff}"/>
                                <Interactions:ChangePropertyAction TargetObject="{Binding ElementName=CloseRamp}" PropertyName="Foreground" Value="{StaticResource NoDiff}"/>
                            </Interactions:DataTriggerBehavior>
                        </Interactivity:Interaction.Behaviors>

                        <StackPanel Style="{StaticResource ResourceKey=Percent}" Visibility="{Binding CloseRampVisibility}">
                            <TextBlock x:Name="CloseRampPercent" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                               Text="{Binding MaximumRampUpPercent}"/>
                        </StackPanel>
                        <TextBlock x:Name="CloseRamp" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                               Text="{Binding MaximumRampUp}" Style="{StaticResource MainValue}" Visibility="{Binding CloseRampVisibility}"/>
                    </StackPanel>
                </DataTemplate>
            </StackPanel.Resources>
            <StackPanel Style="{StaticResource ResourceKey=Label}">
                <TextBlock x:Name="labelCloseRamp" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                           Text="Maximum ramp-up" Width="150"/>
            </StackPanel>
            <ContentControl ContentTemplate="{StaticResource rowBody}"/>

            <ItemsControl ItemsSource="{Binding Alts}" >
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Horizontal"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource AltBlock}">
                            <ContentControl ContentTemplate="{StaticResource rowBody}"/>
                        </StackPanel>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
            
        </StackPanel>
        <StackPanel Orientation="Horizontal" Visibility="{Binding BaseDamageVisibility}">
            <StackPanel.Resources>
                <DataTemplate x:Key="rowBody">
                    <StackPanel Orientation="Horizontal">
                        <Interactivity:Interaction.Behaviors>
                            <Interactions:DataTriggerBehavior Binding="{Binding BaseDamageDiff}" ComparisonCondition="Equal" Value="False">
                                <Interactions:ChangePropertyAction TargetObject="{Binding ElementName=BaseRampPercent}" PropertyName="Foreground" Value="{StaticResource NoDiff}"/>
                                <Interactions:ChangePropertyAction TargetObject="{Binding ElementName=BaseRamp}" PropertyName="Foreground" Value="{StaticResource NoDiff}"/>
                            </Interactions:DataTriggerBehavior>
                        </Interactivity:Interaction.Behaviors>
                        <StackPanel Style="{StaticResource ResourceKey=Percent}" Visibility="{Binding BaseDamageVisibility}">
                            <TextBlock x:Name="BaseRampPercent" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                                           Text="{Binding BaseDamagePercent}" />
                        </StackPanel>
                        <TextBlock x:Name="BaseRamp" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                                       Text="{Binding BaseDamage}" Style="{StaticResource MainValue}" Visibility="{Binding BaseDamageVisibility}"/>
                    </StackPanel>
                </DataTemplate>
            </StackPanel.Resources>
            <StackPanel Style="{StaticResource ResourceKey=Label}">
                <TextBlock x:Name="labelBaseRamp" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                           Text="Base damage" Width="150"/>
            </StackPanel>
            <ContentControl ContentTemplate="{StaticResource rowBody}"/>


            <ItemsControl ItemsSource="{Binding Alts}" >
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Horizontal"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <StackPanel Orientation="Horizontal" Style="{StaticResource AltBlock}">
                            <ContentControl ContentTemplate="{StaticResource rowBody}"/>
                        </StackPanel>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
            
        </StackPanel>
        
