        <!-- Minimum splash 	50% 	83.82 cm-->
        <StackPanel Orientation="Horizontal" Visibility="{Binding MinimumSplashVisibility}">
            <StackPanel.Resources>
                <DataTemplate x:Key="rowBody">
                    <StackPanel Orientation="Horizontal">
                        <Interactivity:Interaction.Behaviors>
                            <Interactions:DataTriggerBehavior Binding="{Binding MinimumSplashDiff}" ComparisonCondition="Equal" Value="False">
                                <Interactions:ChangePropertyAction TargetObject="{Binding ElementName=MinimumSplashRampPercent}" PropertyName="Foreground" Value="{StaticResource NoDiff}"/>
                                <Interactions:ChangePropertyAction TargetObject="{Binding ElementName=MinimumSplashRamp}" PropertyName="Foreground" Value="{StaticResource NoDiff}"/>
                            </Interactions:DataTriggerBehavior>
                        </Interactivity:Interaction.Behaviors>
                        <StackPanel Style="{StaticResource ResourceKey=Percent}" Visibility="{Binding MinimumSplashVisibility}">
                            <TextBlock x:Name="MinimumSplashRampPercent" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                                           Text="{Binding MinimumSplashDamagePercent, FallbackValue=50%}" />
                        </StackPanel>
                        <TextBlock x:Name="MinimumSplashRamp" HorizontalAlignment="Center" TextWrapping="Wrap" VerticalAlignment="Center" 
                                       Text="{Binding MinimumSplashDamage}" Style="{StaticResource MainValue}" Visibility="{Binding MinimumSplashVisibility}"/>
                    </StackPanel>
                </DataTemplate>
